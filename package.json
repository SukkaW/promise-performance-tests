{
  "name": "promise-performance-tests",
  "version": "0.0.1",
  "description": "Promise performance tests",
  "main": "run.js",
  "scripts": {
    "lint": "eslint lib/*.js benchmark/*.js run.js webpack.config.js --fix",
    "build:doxbee-async-tsc": "node tsc.js 'lib/doxbee-async.js' 'build/doxbee-async-tsc.js'",
    "build:doxbee-async-babel": "babel -o build/doxbee-async-babel.js --presets=@babel/preset-env lib/doxbee-async.js",
    "build:fibonacci-async-tsc": "node tsc.js 'lib/fibonacci-async.js' 'build/fibonacci-async-tsc.js'",
    "build:fibonacci-async-babel": "babel -o build/fibonacci-async-babel.js --plugins @babel/plugin-proposal-async-generator-functions --presets=@babel/preset-env lib/fibonacci-async.js",
    "build:measure-async-tsc": "node tsc.js 'lib/measure-async.js' 'build/measure-async-tsc.js'",
    "build:measure-async-babel": "babel -o build/measure-async-babel.js --presets=@babel/preset-env lib/measure-async.js",
    "build:parallel-async-tsc": "node tsc.js 'lib/parallel-async.js' 'build/parallel-async-tsc.js'",
    "build:parallel-async-babel": "babel -o build/parallel-async-babel.js --presets=@babel/preset-env lib/parallel-async.js",
    "build": "npm run build:doxbee-async-tsc && npm run build:doxbee-async-babel && npm run build:fibonacci-async-tsc && npm run build:fibonacci-async-babel && npm run build:measure-async-tsc && npm run build:measure-async-babel && npm run build:parallel-async-tsc && npm run build:parallel-async-babel && webpack",
    "postinstall": "npm run build",
    "start": "node run.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": {
    "name": "Benedikt Meurer",
    "email": "bmeurer@chromium.org",
    "url": "http://benediktmeurer.de"
  },
  "license": "Apache License 2.0",
  "dependencies": {
    "@sukka/listdir": "^0.2.0",
    "bluebird": "^3.5.1",
    "core-js-pure": "^3.14.0",
    "es6-promise": "^4.2.8",
    "promise": "^8.1.0",
    "promise-polyfill": "^8.2.0",
    "regenerator-runtime": "^0.11.1",
    "text-table": "^0.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "eslint": "^7.28.0",
    "eslint-config-sukka": "^1.1.2",
    "eslint-plugin-node": "^11.1.0",
    "typescript": "^4.4.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.2"
  },
  "engines": {
    "node": ">=12"
  }
}
